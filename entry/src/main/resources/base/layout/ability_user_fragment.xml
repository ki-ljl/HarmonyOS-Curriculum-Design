<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:background_element="#C9C969"
    ohos:orientation="vertical">

    <DependentLayout
        ohos:width="match_parent"
        ohos:height="match_content">

        <Image
            ohos:id="$+id:h_back"
            ohos:width="match_parent"
            ohos:height="200vp"/>

        <Image
            ohos:id="$+id:h_head"
            ohos:width="100vp"
            ohos:height="100vp"
            ohos:center_in_parent="true"
            ohos:image_src="$media:ic_irving"
            ohos:scale_mode="clip_center"/>

        <DependentLayout
            ohos:width="match_parent"
            ohos:height="match_content"
            ohos:align_bottom="$id:h_back"
            ohos:bottom_margin="20vp"
            ohos:orientation="horizontal">

            <Image
                ohos:id="$+id:user_line"
                ohos:width="1vp"
                ohos:height="25vp"
                ohos:horizontal_center="true"
                ohos:left_margin="15vp"
                ohos:background_element="#FFFFFF" />

            <Text
                ohos:id="$+id:user_class"
                ohos:width="match_content"
                ohos:height="23vp"
                ohos:left_of="$id:user_line"
                ohos:text="计算1801"
                ohos:text_color="#FFFFFF"
                ohos:text_size="17fp" />

            <Text
                ohos:id="$+id:user_name"
                ohos:width="match_content"
                ohos:height="23vp"
                ohos:left_margin="15vp"
                ohos:right_of="$id:user_line"
                ohos:text="李俊良"
                ohos:text_color="#FFFFFF"
                ohos:text_size="17fp" />
        </DependentLayout>

    </DependentLayout>

    <DependentLayout
